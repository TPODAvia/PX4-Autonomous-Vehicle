<?xml version='1.0'?>
<sdf version='1.4'>

  <model name="diff_rover">


    <link name='base_link1'>
      <pose>0 0 0.1 0 0 0</pose>
  
      <inertial>
        <pose>0 0 0 0 -0 0</pose>
        <mass>20.0</mass>
        <inertia>
          <ixx>0.37083</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.37083</iyy>
          <iyz>0.0</iyz>
          <izz>0.53333</izz>
        </inertia>
      </inertial>

      <collision name='collision'>
        <geometry>
          <box>
            <size>0.4 <div class="5"></div> 0.1</size>
          </box>
        </geometry>
      </collision>

      <visual name='visual'>
        <geometry>
          <box>
            <size>0.4 0.5 0.1</size>
          </box>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Grey</name>
            <uri>__default__</uri>
          </script>
        </material>
      </visual>

      <collision name='caster_collision1'>
        <pose>-0.15 0 -0.05 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>.05</radius>
          </sphere>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1.0</mu>
              <mu2>0.45</mu2>
              <slip1>1.0</slip1>
              <slip2>1.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>

      <visual name='caster_visual1'>
        <pose>-0.15 0 -0.05 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>.05</radius>
          </sphere>
        </geometry>
        <material>
          <script>
            <name>Gazebo/DarkGrey</name>
            <uri>__default__</uri>
          </script>
        </material>
      </visual>

    </link>















    <!-- Left Wheel -->

    <link name="left_wheel">
      <pose>0.1 0.33 0.1 0 1.5707 1.5707</pose>

      <inertial>
        <pose>0 0 0 1.57079632679 0 0</pose>
        <mass>0.414</mass>
        <inertia>
          <ixx>0.0068682</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0068682</iyy>
          <iyz>0</iyz>
          <izz>0.0097299</izz>
        </inertia>
      </inertial>

      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>.1</radius>
            <length>.05</length>
          </cylinder>
        </geometry>
          <surface>
            <friction>
              <torsional>
                <coefficient>0.25</coefficient>
                <use_patch_radius>1</use_patch_radius>
                <surface_radius>0.0686</surface_radius>
              </torsional>
              <ode>
                <mu>1.0</mu>
                <mu2>0.45</mu2>
                <fdir1>1 0 0</fdir1>
                <slip1>0.0</slip1>
                <slip2>0.0</slip2>
              </ode>
            </friction>
            <bounce>
              <restitution_coefficient>0</restitution_coefficient>
              <threshold>1e6</threshold>
            </bounce>
            <contact>
              <ode>
                <min_depth>0.001</min_depth>
                <max_vel>0.0</max_vel>
                <kp>1.0e6</kp>
                <kd>100.0</kd>
              </ode>
            </contact>
          </surface>
      </collision>

      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>.1</radius>
            <length>.05</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <name>Gazebo/DarkGrey</name>
            <uri>__default__</uri>
          </script>
        </material>
      </visual>

    </link>

    <joint type="revolute" name="left_wheel_hinge">
      <pose>0 0 -0.03 0 0 0</pose>
      <child>left_wheel</child>
      <parent>base_link1</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>
















    <!-- Right Wheel -->
    <link name="right_wheel">
      <pose>0.1 -0.33 0.1 0 1.5707 1.5707</pose>

      <inertial>
        <pose>0 0 0 1.57079632679 0 0</pose>
        <mass>0.414</mass>
        <inertia>
          <ixx>0.0068682</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0068682</iyy>
          <iyz>0</iyz>
          <izz>0.0097299</izz>
        </inertia>
      </inertial>

      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>.1</radius>
            <length>.05</length>
          </cylinder>
        </geometry>
          <surface>
            <friction>
              <torsional>
                <coefficient>0.25</coefficient>
                <use_patch_radius>1</use_patch_radius>
                <surface_radius>0.0686</surface_radius>
              </torsional>
              <ode>
                <mu>1.0</mu>
                <mu2>0.45</mu2>
                <fdir1>1 0 0</fdir1>
                <slip1>0.0</slip1>
                <slip2>0.0</slip2>
              </ode>
            </friction>
            <bounce>
              <restitution_coefficient>0</restitution_coefficient>
              <threshold>1e6</threshold>
            </bounce>
            <contact>
              <ode>
                <min_depth>0.001</min_depth>
                <max_vel>0.0</max_vel>
                <kp>1.0e6</kp>
                <kd>100.0</kd>
              </ode>
            </contact>
          </surface>
      </collision>

      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>.1</radius>
            <length>.05</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <name>Gazebo/DarkGrey</name>
            <uri>__default__</uri>
          </script>
        </material>
      </visual>

    </link>

    <joint type="revolute" name="right_wheel_hinge">
      <pose>0 0 0.03 0 0 0</pose>
      <child>right_wheel</child>
      <parent>base_link1</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>

















    <!-- GPS Sensors -->
  
    <include>
        <uri>model://gps</uri>
        <pose>0.1 0 0.2 0 0 0</pose>
        <name>gps</name>
    </include>

    <joint name='gps_joint' type='fixed'>
        <child>gps::link</child>
        <parent>base_link1</parent>
    </joint>




    <!-- IMU -->
    <link name='rover/imu_link'>

      <pose>0.1 0 0.2 0 0 0</pose>

      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.015</mass>
        <inertia>
          <ixx>1e-05</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1e-05</iyy>
          <iyz>0</iyz>
          <izz>1e-05</izz>
        </inertia>
      </inertial>

      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.04</radius>
            <length>0.004</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Orange</name>
          </script>
        </material>
      </visual>

    </link>

    <joint name='standard_vtol/imu_joint' type='revolute'>
      <child>rover/imu_link</child>
      <parent>base_link1</parent>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
          <effort>0</effort>
          <velocity>0</velocity>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>

    <plugin name='gazebo_imu_plugin' filename='libgazebo_imu_plugin.so'>
      <robotNamespace></robotNamespace>
      <linkName>rover/imu_link</linkName>
      <imuTopic>/imu</imuTopic>
      <gyroscopeNoiseDensity>0.0003394</gyroscopeNoiseDensity>
      <gyroscopeRandomWalk>3.8785e-05</gyroscopeRandomWalk>
      <gyroscopeBiasCorrelationTime>1000.0</gyroscopeBiasCorrelationTime>
      <gyroscopeTurnOnBiasSigma>0.0087</gyroscopeTurnOnBiasSigma>
      <accelerometerNoiseDensity>0.004</accelerometerNoiseDensity>
      <accelerometerRandomWalk>0.006</accelerometerRandomWalk>
      <accelerometerBiasCorrelationTime>300.0</accelerometerBiasCorrelationTime>
      <accelerometerTurnOnBiasSigma>0.196</accelerometerTurnOnBiasSigma>
    </plugin>




    <!-- Ground Truth -->

    <plugin name='groundtruth_plugin' filename='libgazebo_groundtruth_plugin.so'>
      <robotNamespace/>
    </plugin>


    <!-- Magnetometer -->

    <plugin name='magnetometer_plugin' filename='libgazebo_magnetometer_plugin.so'>
      <robotNamespace/>
      <pubRate>100</pubRate>
      <noiseDensity>0.0004</noiseDensity>
      <randomWalk>6.4e-06</randomWalk>
      <biasCorrelationTime>600</biasCorrelationTime>
      <magTopic>/mag</magTopic>
    </plugin>


    <!-- Barometer -->

    <plugin name='barometer_plugin' filename='libgazebo_barometer_plugin.so'>
      <robotNamespace/>
      <pubRate>50</pubRate>
      <baroTopic>/baro</baroTopic>
    </plugin>


    <!-- Lidar -->

    <link name="lidar_link">
      <pose>0 0 0.22 0 0 0</pose>
      <gravity>false</gravity>

      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.015</mass>
        <inertia>
          <ixx>1e-05</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1e-05</iyy>
          <iyz>0</iyz>
          <izz>1e-05</izz>
        </inertia>
      </inertial>

      <visual name="visual">
        <geometry>
          <box>
            <size>0.01 0.01 0.01</size>
          </box>
        </geometry>
      </visual>

      <sensor name="laser" type="ray">
        <pose>0 0 0 0 -0 0</pose>
        <ray>
          <scan>
            <horizontal>
              <samples>360</samples>
              <resolution>1</resolution>
              <min_angle>0.0</min_angle>
              <max_angle>6.28319</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.08</min>
            <max>10</max>
            <resolution>0.015</resolution>
          </range>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.01</stddev>
          </noise>
        </ray>
        <plugin name="laser" filename="libgazebo_ros_laser.so"> 
        <topicName>/base_scan</topicName> 
        <frameName>lidar</frameName> </plugin> 
        <always_on>1</always_on>
        <update_rate>30</update_rate>
        <visualize>false</visualize>
      </sensor>

