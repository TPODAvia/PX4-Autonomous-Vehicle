        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>
















    <!-- Right Bottom Wheel -->

    <link name="right_bottom_wheel">
      <pose>0 -0.12 0 1.57 0 0</pose>

      <inertial>
        <pose>0 0 0 1.57 0 0</pose>
        <mass>0.4</mass>
        <inertia>
          <ixx>0.0003</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0003</iyy>
          <iyz>0</iyz>
          <izz>0.0004</izz>
        </inertia>
      </inertial>

      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.06</radius>
            <length>0.01</length>
          </cylinder>
        </geometry>
          <surface>
            <friction>
              <torsional>
                <coefficient>0.25</coefficient>
                <use_patch_radius>1</use_patch_radius>
                <surface_radius>0.0686</surface_radius>
              </torsional>
              <ode>
                <mu>1.0</mu>
                <mu2>0.45</mu2>
                <fdir1>1 0 0</fdir1>
                <slip1>0.0</slip1>
                <slip2>0.0</slip2>
              </ode>
            </friction>
            <bounce>
              <restitution_coefficient>0</restitution_coefficient>
              <threshold>1e6</threshold>
            </bounce>
            <contact>
              <ode>
                <min_depth>0.001</min_depth>
                <max_vel>0.0</max_vel>
                <kp>1.0e6</kp>
                <kd>100.0</kd>
              </ode>
            </contact>
          </surface>
      </collision>

      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.06</radius>
            <length>0.01</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <name>Gazebo/DarkGrey</name>
            <uri>__default__</uri>
          </script>
        </material>
      </visual>

    </link>

    <joint type="revolute" name="right_bottom_wheel_hinge">
      <pose>0 0 0 0 0 0</pose>
      <child>right_bottom_wheel</child>
      <parent>base_robot</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>

























