      <pose>0 0 0 0 0 0</pose>
      <child>lidar_link</child>
      <parent>base_link</parent>
    </joint>




















    <!--FPV Camera -->

    <link name='camera_link'>
      <pose>0.1 0 0.08 0 0 0</pose>

      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.015</mass>
        <inertia>
          <ixx>1e-05</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1e-05</iyy>
          <iyz>0</iyz>
          <izz>1e-05</izz>
        </inertia>
      </inertial>

      <visual name='visual'>
        <geometry>
          <box>
            <size>0.01 0.01 0.01</size>
          </box>
        </geometry>
      </visual>

      <sensor type="camera" name="main_camera">
        <pose>0 0 0 0 0 0</pose>
        <camera>
          <horizontal_fov>1.6075201319067056</horizontal_fov>
          <image>
            <format>B8G8R8</format>
            <width>320</width>
            <height>240</height>
          </image>
          <clip>
            <near>0.02</near>
            <far>1000</far>
          </clip>
          <distortion>
            <k1>-0.273377</k1>
            <k2>0.0642871</k2>
            <k3>-0.00599387</k3>
            <p1>-0.00086158</p1>
            <p2>-0.000443529</p2>
          </distortion>
        </camera>
        <always_on>1</always_on>
        <update_rate>40</update_rate>
        <visualize>1</visualize>
        <plugin name="camera_plugin" filename="libgazebo_ros_camera.so">
          <alwaysOn>true</alwaysOn>
          <imageTopicName>image_raw</imageTopicName>
          <cameraTopicName>camera_info</cameraTopicName>
          <updateRate>40</updateRate>
          <cameraName>main_camera</cameraName>
          <frameName>main_camera_optical</frameName>
          <CxPrime>158.0735</CxPrime>
          <Cx>158.0735</Cx>
          <Cy>108.513</Cy>
          <!-- <focalLength>????</focalLength> -->
          <distortionK1>0.0</distortionK1>
          <distortionK2>0.0</distortionK2>
          <distortionK3>0.0</distortionK3>
          <distortionT1>0.0</distortionT1>
          <distortionT2>0.0</distortionT2>
          <!-- throttling_camera specific options start here -->
          <minUpdateRate>38</minUpdateRate>
          <windowSize>40</windowSize>
          <maxStDev>0.075</maxStDev>
        </plugin>
        <plugin name="GstCameraPlugin" filename="libgazebo_gst_camera_plugin.so">
            <robotNamespace></robotNamespace>
            <udpHost>127.0.0.1</udpHost>
            <udpPort>5600</udpPort>
        </plugin>

      </sensor>
      <self_collide>0</self_collide>
      <kinematic>0</kinematic>
    </link>

    <joint type="fixed" name="camera_joint">
      <parent>base_link</parent>
      <child>camera_link</child>
      <pose>0 0 0 0 0 0</pose>
      <axis>
        <xyz>0 0 1</xyz>
      </axis>
    </joint>
















